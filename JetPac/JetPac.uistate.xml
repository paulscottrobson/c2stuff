<?xml version="1.0" encoding="utf-8" ?>
<c2uistate>
    <!--This is a UI state file - its content describes the user interface settings when the project was last open.
It is entirely optional and the project will load without it.  If you are placing the project under source control,
you probably do not want to add any .uistate.xml files to the repository.-->
    <configurations>
        <active>
            <configuration name="HTML5" />
        </active>
        <display name="HTML5" />
    </configurations>
    <open-tabs>
        <layout order="0">Main Game</layout>
        <event-sheet order="1">Game Events</event-sheet>
        <event-sheet order="2">Player Movement</event-sheet>
        <event-sheet order="3">Controllers</event-sheet>
        <event-sheet order="4">System</event-sheet>
        <event-sheet order="5">Carriables</event-sheet>
        <event-sheet active="1" order="6">Enemies</event-sheet>
        <event-sheet order="7">Globals</event-sheet>
    </open-tabs>
    <properties>
        <property expanded="1" name="About" />
        <property expanded="1" name="Project settings" />
        <property expanded="0" name="Window Size" />
        <property expanded="1" name="Configuration Settings" />
    </properties>
    <families-expanded root="1">
        <family expanded="1" name="Platforms" />
        <family expanded="1" name="Carriable" />
    </families-expanded>
    <folders-expanded>
        <folder expanded="1" path="object-folder/" />
        <folder expanded="1" path="layout-folder/" />
        <folder expanded="1" path="event-folder/" />
        <folder expanded="1" path="file-folder/" />
        <folder expanded="1" path="sound-folder/" />
        <folder expanded="1" path="music-folder/" />
        <folder expanded="0" path="file-folder/Icons/" />
        <folder expanded="1" path="animation-folder/PlatformLeft/" />
        <folder expanded="1" path="animation-folder/PlatformRight/" />
        <folder expanded="1" path="animation-folder/PlatformCentre/" />
        <folder expanded="1" path="animation-folder/LivesSprite/" />
        <folder expanded="1" path="animation-folder/Player/" />
        <folder expanded="1" path="animation-folder/Laser/" />
        <folder expanded="1" path="animation-folder/Rocket/" />
        <folder expanded="1" path="animation-folder/Component/" />
        <folder expanded="1" path="animation-folder/FuelBar/" />
        <folder expanded="1" path="animation-folder/Collectable/" />
        <folder expanded="1" path="animation-folder/Enemy/" />
        <folder expanded="1" path="animation-folder/Explosion/" />
    </folders-expanded>
</c2uistate>
